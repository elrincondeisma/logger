{
    "name": "icatala/logger",
    "description": "Logger - A simple package for logging in Laravel.",
    "keywords": [
        "icatala",
        "logger"
    ],
    "homepage": "https://github.com/icatala/logger",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ismael Catala Gil",
            "email": "catalagil.ismael@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Icatala\\Logger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icatala\\Logger\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Icatala\\Logger\\LoggerServiceProvider"
            ],
            "aliases": {
                "Logger": "Icatala\\Logger\\LoggerFacade"
            }
        }
    }
}
